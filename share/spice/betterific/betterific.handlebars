{{#if betterifs.betterifs}}
  <div class="kind-wrapper">
    <div class="betterifs">
      {{#each betterifs.betterifs}}
        {{{include "betterif"}}}
      {{/each}}
    </div>
  </div>
{{/if}}
{{#if tags.tags}}
  {{#if betterifs.betterifs}}
    <hr/>
  {{/if}}
  <div class="kind-wrapper">
    <a href="#" class="header">Tags</a>
    <div class="tags{{#if betterifs.betterifs}} collapsible{{/if}}">
      {{#each tags.tags}}
        {{{include "tag"}}}
      {{/each}}
    </div>
  </div>
{{/if}}
{{#if users.users}}
  {{#if betterifs.betterifs}}
    <hr/>
  {{else}}
    {{#if tags.tags}}
      <hr/>
    {{/if}}
  {{/if}}
  <div class="kind-wrapper">
    <a href="#" class="header">Users</a>
    <div class="users{{#if betterifs.betterifs}} collapsible{{else}}{{#if tags.tags}} collapsible{{/if}}{{/if}}">
      {{#each users.users}}
        {{{include "user"}}}
      {{/each}}
    </div>
  </div>
{{/if}}
